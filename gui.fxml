<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets top="40.0" right="40.0" bottom="40.0" left="40.0" />
    </padding>
    <children>
        <Button fx:id="userProfileBtn" mnemonicParsing="false" text="User Profile" textFill="WHITE">
            <font>
                <Font size="30.0" />
            </font>
        </Button>
        <Button fx:id="workoutsBtn" mnemonicParsing="false" text="Workouts" textFill="WHITE">
            <font>
                <Font size="30.0" />
            </font>
        </Button>
    </children>
</VBox>
